<h1> Ici la liste des restaurants </h1>

   <% @restaurants.each do |restaurant| %>
    <h4> <%= link_to restaurant.name, restaurant_path(restaurant) %> </h4>
    <h5> <%= restaurant.address %> </h5>
    <h5> <%= restaurant.phone_number %> </h5>
    <h5> <%= restaurant.category %> </h5>

    <%= link_to "Add a review", new_restaurant_review_path(restaurant) %>
   <% end %>

<%= link_to "create a new restaurant", new_restaurant_path %>
